generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model lead {
  id Int @id @default(autoincrement())

  firstName     String
  lastName      String
  email         String
  jobTitle      String?
  countryCode   String?
  companyName   String?
  message       String?
  emailVerified Boolean?
  phone         String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
